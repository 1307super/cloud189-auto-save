{
    "manifest_version": 3,
    "name": "Cloud 189 转存插件",
    "version": "1.0",
    "description": "在 cloud.189.cn 页面添加转存功能",
    "permissions": ["activeTab", "storage", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png"
        }
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    }
}
    